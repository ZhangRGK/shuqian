{
    "name": "bigmarks",
    "version": "1.0",
    "manifest_version": 3,
    "description": "用来和书签同步收藏的书签",
    "browser_action": {
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["bigmarks.js"]
    },
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "bookmarks",
        "http://0.0.0.0:3000/"
        ]
}
